{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DatabaseSettings": {
    "CollectionName": "MT4040_VoterFramework",
    "ConnectionString": "mongodb://w3.training5.modak.com:27017/",
    "DatabaseName": "training_2022"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },

    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "{Timestamp: G}{Message }{NewLine:1}{Exception:1}",
          "path": "Logs\\operational\\log-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "UserSettings": {
    "AuditLog": {
      "Enable": true,
      "Path": "Logs\\audit\\log-.log",
      "RollingInterval": "Day",
      "Shared": true,
      "RetainedFileCountLimit": 10
    },
    "BasePath": "/training_2022",
    "EnableMiniProfiler": true
  }
}